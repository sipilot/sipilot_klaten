<%#= render 'shareds/section_title', title_name: 'Permohonan' %>
<%#= render 'shareds/tables/filter_n_search_by_date_location', action_data: @search, action_path: gps_admin_gps_members_path %>

<div class="bg-white px-4 py-5 border-b border-gray-200 sm:px-6">
  <div class="-ml-4 -mt-2 flex items-center justify-between flex-wrap sm:flex-nowrap">
    <div class="ml-4 mt-2">
        <h3 class="text-lg leading-6 font-medium text-gray-900">
            Workspace : <%= @workspace.name %>
        </h3>
        <h2 class="text-lg leading-6 font-medium text-gray-900 pt-2">
            Kecamatan : <%= @workspace.sub_district_name %>
        </h3>
        <h3 class="text-lg leading-6 font-medium text-gray-900 pt-2">
            Desa : <%= @workspace.village_name %>
        </h3>
    </div>
    <div class="ml-4 mt-2 flex-shrink-0">
      <%= link_to 'Kembali', 
        intip_admin_intip_members_detail_path(@member),
        class: "relative inline-flex items-center px-4 py-2 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-blue-600 hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500"
      %>
    </div>
  </div>
</div>

<%= render 'intips/table_filter' %>

<div class="p-4 bg-white border-b border-gray-200 sm:flex sm:items-center sm:justify-between ">
  <div class="mt-3 flex sm:mt-0 sm:ml-4">
  </div>

  <div class="mt-3 flex sm:mt-0 sm:ml-4">

    <%=
      render 'shareds/tables/button_download',
      shp_path: intip_admin_intip_members_permohonan_path('q[created_at_gteq]' => @gteq_params, 'q[created_at_lteq]' => @lteq_params, 'archive' => true),
      xlsx_path: intip_admin_intip_members_permohonan_path(format: :xlsx)
    %>

  </div>
</div>

  <div class="overflow-x-auto sm:-mx-6 lg:-mx-8 py-1">
    <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
      <div class="shadow overflow-hidden border-b border-gray-200 sm:rounded-lg">

        <table class="min-w-full divide-y divide-gray-200">
          <thead class="bg-gray-50">
            <tr>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                No
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'NUI', column: 'nui'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'NIB', column: 'nib'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Nama Instansi Pemerintah', column: 'name'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Kecamatan', column: 'sub_district_name'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Desa', column: 'village_name'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Alas Hak', column: 'hak_name'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Jenis Hak', column: 'hak_type'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Nomer Hak / Nomer Letter C', column: 'number_letter_c:'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Tanggal', column: 'date'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Jangka Waktu', column: 'time_range'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Penguasaan Tanah', column: 'land_control'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Peruntukan Tanah', column: 'land_allocation'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Penggunaan Tanah', column: 'land_use'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Pemanfaatan Tanah', column: 'land_utilization'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Luas (M2)', column: 'land_size'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Lat', column: 'lattitude'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Lng', column: 'longitude'%>
              </th>
              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "intips/column_header", title: 'Keterangan', column: 'description'%>
              </th>
            </tr>
          </thead>
          <tbody>
            <% @intips.each.with_index(1) do |intip, index|%>
              <tr class="bg-white">
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= index %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.nui %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.nib %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.name %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.sub_district_name %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.village_name %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.hak_number %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.hak_name %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.number_letter_c %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= Formatter.date_dmy(intip.date) %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.time_range %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.land_control %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.land_allocation %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.land_use %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.land_utilization %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.land_size %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.lattitude %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.longitude %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                  <%= intip.description %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>

  <%= render "intips/pagination", name: '' %>
  