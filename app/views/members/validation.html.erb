<%= render "shareds/section_title", title_name: 'Permohonan Pra Pelayanan Validasi'%>
<%= render 'members/table_filter', action_data: @search, action_path: dashboards_member_validasi_path %>

<div class="p-4 bg-white border-b border-gray-200 sm:flex sm:items-center sm:justify-between ">
  <div class="mt-3 flex sm:mt-0 sm:ml-4">
  </div>

  <div class="mt-3 flex sm:mt-0 sm:ml-4">

    <%=
      render 'shareds/tables/button_download',
      xlsx_path: dashboards_member_validasi_path(format: :xlsx, 'q[created_at_gteq]' => @gteq_params, 'q[created_at_lteq]' => @lteq_params),
      pdf_path: dashboards_member_validasi_path(format: :pdf, 'q[created_at_gteq]' => @gteq_params, 'q[created_at_lteq]' => @lteq_params)
    %>

    <%= render 'shareds/tables/button_submission', action_path: dashboards_member_validasi_submission_path %>

  </div>
</div>


<!-- This example requires Tailwind CSS v2.0+ -->
<div class="flex flex-col py-5">
  <div class="-my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
    <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
      <div class="overflow-hidden sm:rounded-lg">
        <table class="order-column stripe hover">
          <thead>
            <tr>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                No
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Kode", column: 'submission_code' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Nomor Hak", column: 'hak_number' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Nama Pendaftar", column: 'on_behalf' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Bertindak Atas", column: 'act_for' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Nama Pemilik Sertipikat", column: 'fullname' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Jenis Hak", column: 'hak_type_name' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Desa", column: 'village_name' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Status Surat Ukur", column: 'submission_status' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Status Buku Tanah", column: 'land_book_status' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Status Kasubsi Tematik", column: 'kasubsi_tematik_status' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                <%= render "members/column_header", title: "Status Kasubsi Pendaftaran", column: 'kasubsi_registration_status' %>
              </th>
              <th scope="col" class="px-6 py-3 bg-gray-50 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                Aksi
              </th>
            </tr>
          </thead>
          <tbody class="bg-white divide-y divide-gray-200">
            <% @submissions.each.with_index(@start_from) do |submission, index| %>
              <tr>
                <td class="text-center">
                  <span class="text-sm text-gray-900"><%= index %></span>
                </td>
                <td class="text-center">
                  <span class="text-sm text-gray-900"><%= submission.submission_code %></span>
                </td>
                <td class="text-center">
                  <span class="text-sm text-gray-900"><%= submission.hak_number %></span>
                </td>
                <td class="text-center">
                  <span class="text-sm text-gray-900"><%= submission.on_behalf %></span>
                </td>
                <td class="text-center">
                  <span class="text-sm text-gray-900"><%= submission.act_for %></span>
                </td>
                <td class="text-center">
                  <span class="text-sm text-gray-900"><%= submission.fullname %></span>
                </td>
                <td class="text-center">
                  <span class="text-sm text-gray-900"><%= submission.hak_name %></span>
                </td>
                <td class="text-center">
                  <span class="text-sm text-gray-900"><%= submission.village_name %></span>
                </td>
                <td class="text-center">
                  <%= render 'shareds/tables/status', status: submission.submission_status %>
                </td>
                <td class="text-center">
                  <%= render 'shareds/tables/status', status: submission.land_book_status %>
                </td>
                <td class="text-center">
                  <%= render 'shareds/tables/status', status: submission.kasubsi_tematik_status %>
                </td>
                <td class="text-center">
                  <%= render 'shareds/tables/status', status: submission.kasubsi_registration_status %>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">
                  <%= render 'shareds/tables/button_detail', action_path: member_submission_detail_path(submission) %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<%= render "shareds/pagination", data: @submissions, name: 'data' %>